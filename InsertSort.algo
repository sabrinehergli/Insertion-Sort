PROCEDURE InsertionSort(arr : ARRAY_OF INTEGER)
VAR
    i,elt,pos,temp:INTEGER;
BEGIN
    FOR i FROM 1 TO arr.length-1 STEP 1  DO
        pos:=i-1;
        elt:=arr[i];
        WHILE (elt<arr[pos]) AND (pos>=0) DO
            arr[pos+1]:=arr[pos];
            pos:=pos-1;
        END_WHILE
        arr[pos+1]:=elt;
    END_FOR
END